<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
	<class name="com.web.entity.Manager" table="Manager" catalog="web">
		<id name="managerAccount" type="java.lang.String">
			<column name="managerAccount"></column>
		</id>
		<property name="managerPassword" type="java.lang.String">
			<column name="managerPassword"></column>
		</property>
	</class>
</hibernate-mapping>