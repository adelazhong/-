<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
	<class name="com.web.entity.Student" table="Student" catalog="web">
		<id name="studentAccount" type="java.lang.String">
			<column name="studentAccount"></column>
		</id>
		<property name="studentPassword" type="java.lang.String">
			<column name="studentPassword"></column>
		</property>
	</class>
</hibernate-mapping>