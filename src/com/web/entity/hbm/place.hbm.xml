<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
	<class name="com.web.entity.Place" table="Place" catalog="web">
		<id name="placeID" type="java.lang.Integer">
			<column name="placeID"></column>
			<generator class="native"/>  
		</id>
		<property name="placeName" type="java.lang.String" >
			<column name="placeName"></column>
		</property>
		<property name="typeID" type="java.lang.Integer">
			<column name="typeID"></column>
		</property>
	</class>
</hibernate-mapping>