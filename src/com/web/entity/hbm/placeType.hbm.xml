<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
	<class name="com.web.entity.PlaceType" table="PlaceType" catalog="web">
		<id name="placeTypeID" type="java.lang.Integer">
			<column name="placeTypeID"></column>
			<generator class="native"/>  
		</id>
		<property name="placeTypeName" type="java.lang.String" >
			<column name="placeTypeName"></column>
		</property>
	</class>
</hibernate-mapping>